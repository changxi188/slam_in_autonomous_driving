{
  "configurations": {
    "Launch": {
      "adapter": "CodeLLDB",
      "filetypes": [ "cpp", "c", "objc", "rust" ], 
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/bin/test_nn",
        "args": [],
        "cwd": "${workspaceRoot}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    }
  }
}

